{
  "name": "src",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "~18.*",
    "npm": "~9.*"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint './**/*.ts' --ignore-pattern '**/*.d.ts' --quiet",
    "lint-fix": "eslint '**/*.ts' --ignore-pattern '**/*.d.ts' --fix",
    "generate-openapi-ingest": "wireit",
    "start-local-api-ingest": "wireit",
    "start-local-api-front": "wireit",
    "start-frontend": "cd src/frontend && npm run dev",
    "watch-local-api-ingest-watch": "npm run start-local-api-ingest --watch",
    "watch-local-api-front-watch": "npm run start-local-api-front --watch"
  },
  "wireit": {
    "start-local-api-ingest": {
      "command": "esr ./scripts/index.ts -c start-local-api-ingest",
      "files": [
        "scripts.ts",
        "src/backend/api-ingest/**/*.ts",
        "src/backend/lib/**/*.ts"
      ]
    },
    "start-local-api-front": {
      "command": "esr ./scripts/index.ts -c start-local-api-front",
      "service": true,
      "files": [
        "scripts.ts",
        "src/backend/api-front/**/*.ts",
        "src/backend/lib/**/*.ts"
      ]
    },
    "generate-openapi-ingest": {
      "command": "esr ./scripts/index.ts -c generate-openapi-ingest",
      "files": [
        "scripts.ts",
        "src/**/*.ts",
        "OpenAPI.yaml"
      ],
      "output": [
        "dist/**"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-athena": "^3.316.0",
    "@aws-sdk/client-firehose": "^3.316.0",
    "@aws-sdk/client-s3": "^3.317.0",
    "@maxmind/geoip2-node": "^3.5.0",
    "@trpc/client": "^10.13.2 ",
    "@trpc/server": "^10.13.2 ",
    "@types/aws-lambda": "^8.10.115",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^9.0.0",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/parser": "^5.59.6",
    "amazon-cognito-identity-js": "^6.2.0",
    "ansi-colors": "^4.1.3",
    "aws-jwt-verify": "^4.0.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "cors": "^2.8.5",
    "esbuild": "^0.17.8",
    "esbuild-runner": "^2.2.2",
    "esbuild-visualizer": "^0.4.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.18.2",
    "isbot": "^3.6.6",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "mocha": "^10.2.0",
    "node-fetch": "^2.6.9",
    "npm": "^9.4.2",
    "open": "^8.4.2",
    "p-limit": "3.1.0",
    "trpc-openapi": "^1.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "ua-parser-js": "^1.0.33",
    "uuid": "^9.0.0",
    "wireit": "^0.9.5",
    "yargs": "^17.7.1",
    "zod": "^3.20.6"
  }
}
